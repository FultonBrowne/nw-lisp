;; repl by Fulton Browne
(defun (profile)
       (print 'default 'profile)
       (print 'hello user)
)
(defun (prompt) ;; think of this as the $PS1
	(princ "> ")
	(flush (outport)) ;; this is dumb
)
(defun (repl)
	(prompt)
	(setq in (read))
	(if (or (equal in 'exit) (eofp in)) (error 'good 'bye!))
	(print (catch-errors () (eval in)))
	(repl))
(defun (start-repl)
	(print 'FREPLv1.0)
    (profile)
(repl))
